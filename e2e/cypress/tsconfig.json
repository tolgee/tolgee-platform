{
  "compilerOptions": {
    "target": "es2020",
    "lib": [
      "es5",
      "dom",
      "es2020"
    ],
    "types": [
      "cypress",
      "cypress-xpath/src", 
      "cy-verify-downloads",
      "node"
    ],
    "jsx": "react-jsx",
    "noEmit": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "moduleResolution": "Node"
  },

}
