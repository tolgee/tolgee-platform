{
  "$schema": "https://docs.tolgee.io/cli-schema.json",
  "projectId": 1,
  "format": "PROPERTIES_ICU",
  "patterns": ["./emails/**/*.ts?(x)", "./components/**/*.ts?(x)"],
  "extractor": "./.config/extractor.ts",
  "pull": {
    "path": "./i18n",
    "fileStructureTemplate": "messages_{snakeLanguageTag}.{extension}"
  },
  "push": {
    "tagNewKeys": ["email"]
  }
}
