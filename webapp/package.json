  "scripts": {
    "start": "vite",
    "develop": "start",
    "build": "vite build",
    "preview": "vite preview",
    "test": "craco test",
    "schema": "node ./scripts/generate-schemas.js public",
    "billing-schema": "node ./scripts/generate-schemas.js billing",
    "prettier": "prettier --write ./src",
    "eslint": "eslint --ext .ts --ext .tsx --max-warnings 0 --report-unused-disable-directives .",
    "tsc": "npm run --prefix ../library build:types && tsc",
    "unused-exports": "ts-unused-exports tsconfig.json --findCompletelyUnusedFiles --ignoreFiles=GoToDocsButton",
    "tsc:prod": "npm run --prefix ../library build:types && tsc --project tsconfig.prod.json",
    "load-translations": "tolgee pull",
    "check-translations": "tolgee extract check",
    "tag-keys:deprecated": "tolgee tag --filter-not-extracted --filter-tag production --tag deprecated",
    "tag-keys:production": "tolgee tag --filter-extracted --tag production --untag deprecated 'draft: *' --untag-other production",
    "tag-keys": "npm run tag-keys:deprecated && npm run tag-keys:production",
    "prepare": "npm run --prefix .. init-husky && node ./scripts/updateBranchInfo.js && node ./scripts/prepareEe.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
